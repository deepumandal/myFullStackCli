{
  "bin": {
    "initfs": "dist/index.js"
  },
  "name": "initfs",
  "publishConfig": {
    "access": "public"
  },
  "version": "1.0.3",
  "description": "Use my Full Stack CLI to built app pre structure in minutes",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "tsc && tsc-esm-fix dist && node scripts/copyTemplates.mjs",
    "start": "node dist/index.js",
    "dev": "clear && pnpm build && pnpm start",
    "link": "npm link",
    "test": "pnpm build && npm run link",
    "prepare": "husky install",
    "lint": "eslint . --ext .ts --fix",
    "format": "prettier --write .",
    "lint-staged": "lint-staged",
    "spell-check": "cspell '**/*.{ts,js,json,md}'",
    "commit": "bash scripts/quick-commit.sh",
    "commit:force": "bash scripts/quick-commit.sh --no-verify",
    "prepublishOnly": "pnpm build"
  },
  "keywords": [],
  "author": "Deepak Mandal",
  "license": "ISC",
  "dependencies": {
    "@types/gradient-string": "^1.1.6",
    "cac": "^6.7.14",
    "chalk": "4",
    "execa": "^9.5.2",
    "figlet": "^1.8.0",
    "fs-extra": "^11.3.0",
    "globby": "^14.1.0",
    "gradient-string": "2",
    "ora": "^8.2.0",
    "prompts": "^2.4.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@eslint/js": "^9.23.0",
    "@types/figlet": "^1.7.0",
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^9.0.7",
    "@types/node": "^22.13.11",
    "@types/prompts": "^2.4.9",
    "commitlint": "^19.8.0",
    "cspell": "^8.17.5",
    "eslint": "^9.23.0",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.0",
    "prettier": "^3.5.3",
    "ts-node": "^10.9.2",
    "tsc-esm-fix": "^3.1.2",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.27.0"
  },
  "lint-staged": {
    "*.{ts,js}": [
      "pnpm lint",
      "pnpm format"
    ]
  },
  "files": [
    "dist",
    "bin.js"
  ]
}
